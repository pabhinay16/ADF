{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-abhinay"
		},
		"linkedserviceDL_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linkedserviceDL'"
		},
		"linkedGIT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"linkedserviceDL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageadfp1.dfs.core.windows.net/"
		},
		"Event trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4d449fe4-7bd7-49d9-ade5-23f7380a05b8/resourceGroups/RG-ADF/providers/Microsoft.Storage/storageAccounts/storageadfp1"
		},
		"man trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/4d449fe4-7bd7-49d9-ade5-23f7380a05b8/resourceGroups/RG-ADF/providers/Microsoft.Storage/storageAccounts/storageadfp1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-abhinay_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linkedserviceDL_accountKey": {
						"value": "[parameters('linkedserviceDL_accountKey')]"
					},
					"linkedGIT_properties_typeProperties_url": {
						"value": "[parameters('linkedGIT_properties_typeProperties_url')]"
					},
					"linkedserviceDL_properties_typeProperties_url": {
						"value": "[parameters('linkedserviceDL_properties_typeProperties_url')]"
					},
					"Event trigger_properties_typeProperties_scope": {
						"value": "[parameters('Event trigger_properties_typeProperties_scope')]"
					},
					"man trigger_properties_typeProperties_scope": {
						"value": "[parameters('man trigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}